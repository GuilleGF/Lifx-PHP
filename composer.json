{
  "name": "guillegf/lifx-php",
  "description": "PHP client for LIFX HTTP API",
  "keywords": [
    "lifx",
    "api",
    "php"
  ],
  "homepage": "https://github.com/GuilleGF/Lifx-PHP",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Guillermo Gonzalez",
      "email": "guillermogf@gmail.com"
    }
  ],
  "require": {
    "php": "^5.5 || ^7.0",
    "guzzlehttp/guzzle": "^6.2",
    "atrapalo/php-tools": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "5.7.*|6.0.*"
  },
  "autoload": {
    "psr-4": {
      "GuilleGF\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GuilleGF\\Lifx\\Tests\\": "tests/"
    }
  },
  "config": {
    "bin-dir": "bin/"
  }
}
